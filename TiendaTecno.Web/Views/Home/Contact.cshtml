                    <li data-target="#demo" data-slide-to="1"></li>
                    <li data-target="#demo" data-slide-to="2"></li>
                </ul>

                <!-- The slideshow -->
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img src="~/images/Static/La 14.png" class="img-fluid" alt="La 14">
                        <div class="carousel-caption">
                            <h3>LOCAL 217, 219, 237</h3>
                            <p>Contacto 301 5275205</p>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img src="~/images/Static/la estacion.png" class="img-fluid" alt="La Estación">
                        <div class="carousel-caption">
                            <h3>LOCAL 289</h3>
                            <p>Contacto 317 3388292</p>
                        </div>
                    </div>
                    <div class="carousel-item">
